<h2> User books <%= current_user.email %> </h2>
<%= link_to "New Book", new_book_path()%>
<ul>
    <% @books.each do |book| %>
        <li> 
            Titulo: <%= book.title %>
            <%= link_to "Edit", edit_book_path(book.id)%>
            <%= link_to "New Note", new_note_path(), :book_id => book.id %>
            <% ### Ver por quÃ© no anda %>
            <%= button_to 'Delete', book, method: :delete, data: { confirm: 'Seguro que desea eliminar el libro?' } %>
    <% end %>
</ul>